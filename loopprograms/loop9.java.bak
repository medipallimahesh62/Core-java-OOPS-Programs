/*WPT COUNT ALL THE NUMBERS DIVISIBLE BY 4 PRESENT BW 10 TO 30*/ 
class loop9
{
	public static void main(String[] args) 
	{
		int count=0;
		for(int c=10;c<=30;c++)
		{
			if(c%4==0)
			{
		
				count++;
			}
		}
			
				
			

		
			
				System.out.println("TOTAL NUMBERS DIVISIBLE BY 4 BW 10 TO 30 IS" +count);
		
	}
}
